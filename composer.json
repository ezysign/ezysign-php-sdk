{
  "name": "ezysign/phpsdk",
  "description": "EzySign Software development Kit",
  "type": "library",
  "require": {
    "php": "~5.6|~7.0",
    "guzzlehttp/guzzle": "^7.2",
    "firebase/php-jwt": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.6"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "EzySignSdk\\": "src/",
      "GuzzleHttp\\": "vendor/guzzlehttp/",
      "GuzzleHttp\\Psr7\\": "vendor/guzzlehttp/psr7/"
    },
    "files": [
      "src/index.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "EzySignSdk\\Test\\": "tests"
    }
  }
}
